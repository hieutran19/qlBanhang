<!DOCTYPE html>
<html>
<%- include("layouts/head", {title: "Trang Chủ" })%>

    <body>

        <%- include("layouts/header")%>

            <!--	Body	-->
            <div id="body">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <%- include("layouts/menu")%>
                        </div>
                    </div>
                    <div class="row">
                        <div id="main" class="col-lg-8 col-md-12 col-sm-12">
                            <%- include("layouts/slider")%>

                                <!--	Feature Product	-->
                                <div class="products">
                                    <h3>Sản phẩm nổi bật</h3>
                                    <div class="product-list card-deck">
                                        <% for(let product of featured){%>
                                            <%- include("./components/product-item", {product})%>
                                        <% }%>
                                    </div>

                                </div>
                                <!--	End Feature Product	-->


                                <!--	Latest Product	-->
                                <div class="products">
                                    <h3>Sản phẩm mới</h3>
                                    <div class="product-list card-deck">
                                        <% for(let product of latest){%>
                                            <%- include("./components/product-item", {product})%>
                                        <% }%>
                                    </div>
                                </div>
                                <!--	End Latest Product	-->

                        </div>

                        <%- include("layouts/sidebar")%>
                    </div>
                </div>
            </div>
            <!--	End Body	-->

            <%- include("layouts/footer")%>


    </body>

</html>